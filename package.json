{
  "name": "eslint-config-stylish",
  "version": "1.0.0",
  "description": "ESLint configuration that extends the stylish rules",
  "main": "index.js",
  "types": "./config/index.d.ts",
  "exports": {
    ".": {
      "types": "./config/index.d.ts",
      "import": "./config/index.js"
    },
    "./react": {
      "types": "./config/index.d.ts",
      "import": "./config/react.js"
    },
    "./typescript": {
      "types": "./config/index.d.ts",
      "import": "./config/typescript.js"
    }
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "react",
    "typescript"
  ],
  "author": "StyleList94",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/StyleList94/eslint-config-stylish.git"
  },
  "bugs": {
    "url": "https://github.com/StyleList94/eslint-config-stylish/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "eslint": "^9.29.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  },
  "peerDependencies": {
    "@eslint/js": ">=9",
    "eslint": ">=9",
    "eslint-import-resolver-typescript": ">=4",
    "eslint-plugin-import": ">=2",
    "eslint-plugin-jsx-a11y": ">=6",
    "eslint-plugin-react": ">=7",
    "eslint-plugin-react-hooks": ">=5",
    "typescript-eslint": ">=8"
  }
}
